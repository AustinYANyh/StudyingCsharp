        <Grid x:Name="ProCutterZoomGrid" Visibility="Collapsed" Width="1890" Height="620" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="30,260,0,0">
            <Border Width="560" Height="260" HorizontalAlignment="Left" VerticalAlignment="Top" BorderBrush="#FF282828" BorderThickness="1">
                <Grid>
                    <Grid.RowDefinitions>
