                        <Label Content="焦点确认延时:" Width="155" Height="35" Foreground="White" FontSize="16" HorizontalAlignment="Left" VerticalAlignment="Top" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Margin="15,50,0,0"></Label>
                        <Label Content="回原点(输出口):" Width="155" Height="35" Foreground="White" FontSize="16" HorizontalAlignment="Left" VerticalAlignment="Top" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Margin="15,90,0,0"></Label>
                        <Label Content="回原点延时:" Width="145" Height="35" Foreground="White" FontSize="16" HorizontalAlignment="Left" VerticalAlignment="Top" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Margin="15,130,0,0"></Label>
                        <Label Content="复位后焦点位置:" Width="160" Height="35" Foreground="White" FontSize="16" HorizontalAlignment="Left" VerticalAlignment="Top" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Margin="15,173,0,0"/>
                        <TextBox Width="148" Height="33" Tag="ms" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="228,53,0,0"/>
                        <TextBox Width="148" Height="33" Tag="ms" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="228,133,0,0"/>
                        <TextBox Width="148" Height="33" Tag="mm" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="228,173,0,0"/>
                        <ComboBox Width="148" Height="29" FontSize="16" Background="#FFFFFF" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="228,13,0,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" SelectedIndex="0">
                            <ComboBoxItem FontSize="18" VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Content="0"></ComboBoxItem>
                            <ComboBoxItem FontSize="18" VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Content="A1"></ComboBoxItem>
                        </ComboBox>
                        <ComboBox Width="148" Height="29" FontSize="16" Background="#FFFFFF" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="228,93,0,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" SelectedIndex="0">
                            <ComboBoxItem FontSize="18" VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Content="0"/>
                            <ComboBoxItem FontSize="18" VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Content="A1"/>
                        </ComboBox>
                    </Grid>
                </Grid>
            </Border>

            <Border Width="560" Height="326" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,280,0,0" BorderBrush="#FF282828" BorderThickness="1">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="48"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0" Background="Black">
                        <Label Content="焦点曲线" Width="100" Height="35" Foreground="White" FontSize="21" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"></Label>
                    </Grid>
                    <Grid Grid.Row="1">
                        <Image Width="558" Height="278" Source="/BodorLaserWPFConfig;component/Images/Config_Image_FakeFocusCurve.jpg" Stretch="Fill"></Image>
                    </Grid>
                </Grid>
            </Border>
        </Grid>

